{
  "AppSettings": {
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
          "SingleLine": true,
          "IncludeScopes": true,
          "TimestampFormat": "yyyy-MM-dd HH:mm:ss,fff ",
          "UseUtcTimestamp": true,
          "ColorBehavior": 2
      }
    }
  },
  "AllowedHosts": "*"
}
